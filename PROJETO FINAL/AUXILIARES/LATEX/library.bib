  abstract      = {Uma das etapas mais importantes da an{\'{a}}lise de imagens e, que conta com uma enorme quantidade de aplica{\c{c}}{\~{o}}es, {\'{e}} a segmenta{\c{c}}{\~{a}}o. No entanto, uma boa parte das t{\'{e}}cnicas tradicionais apresenta alto custo computacional, dificultando sua aplica{\c{c}}{\~{a}}o em imagens de alta resolu{\c{c}}{\~{a}}o como, por exemplo, as imagens de ninhais de aves do Pantanal que tamb{\'{e}}m ser{\~{a}}o analisadas neste trabalho. Diante disso, {\'{e}} proposta uma nova abordagem de segmenta{\c{c}}{\~{a}}o que combina algoritmos de detec{\c{c}}{\~{a}}o de comunidades, pertencentes {\`{a}} teoria das redes complexas, com t{\'{e}}cnicas de extra{\c{c}}{\~{a}}o de superpixels. Tal abordagem {\'{e}} capaz de segmentar imagens de alta resolu{\c{c}}{\~{a}}o mantendo o compromisso entre acur{\'{a}}cia e tempo de processamento. Al{\'{e}}m disso, como as imagens de ninhais analisadas apresentam caracter{\'{i}}sticas peculiares que podem ser mais bem tratadas por t{\'{e}}cnicas de segmenta{\c{c}}{\~{a}}o por textura, a t{\'{e}}cnica baseada em Markov Random Fields (MRF) {\'{e}} proposta, como um complemento {\`{a}} abordagem de segmenta{\c{c}}{\~{a}}o inicial, para realizar a identifica{\c{c}}{\~{a}}o final das aves. Por fim, devido {\`{a}} import{\^{a}}ncia de avaliar quantitativamente a qualidade das segmenta{\c{c}}{\~{o}}es obtidas, um nova m{\'{e}}trica de avalia{\c{c}}{\~{a}}o baseada em ground-truth foi desenvolvida, sendo de grande import{\^{a}}ncia para a {\'{a}}rea. Este trabalho contribuiu para o avan{\c{c}}o do estado da arte das t{\'{e}}cnicas de segmenta{\c{c}}{\~{a}}o de imagens de alta resolu{\c{c}}{\~{a}}o, aprimorando e desenvolvendo m{\'{e}}todos baseados na combina{\c{c}}{\~{a}}o de redes complexas com superpixels, os quais alcan{\c{c}}aram resultados satisfat{\'{o}}rios com baixo tempo de processamento. Al{\'{e}}m disso, uma importante contribui{\c{c}}{\~{a}}o referente ao censo demogr{\'{a}}fico de aves por meio da an{\'{a}}lise de imagens a{\'{e}}reas de ninhais foi viabilizada por meio da aplica{\c{c}}{\~{a}}o da t{\'{e}}cnica de segmenta{\c{c}}{\~{a}}o MRF.},
  doi           = {10.11606/t.55.2014.tde-16032015-113613},
  keywords      = {Censo demogr{\'{a}}fico de aves,Detec{\c{c}}{\~{a}}o de comunidades,Markov Random fields,Redes complexas,Segmenta{\c{c}}{\~{a}}o de imagens,Super Pixels,Textura},
  mendeley-tags = {Censo demogr{\'{a}}fico de aves,Detec{\c{c}}{\~{a}}o de comunidades,Markov Random fields,Redes complexas,Segmenta{\c{c}}{\~{a}}o de imagens,Super Pixels,Textura},
  pages         = {104},
  publisher     = {Universidade de Sao Paulo, Agencia {USP} de Gestao da Informacao Academica ({AGUIA})},
  url           = {https://www.teses.usp.br/teses/disponiveis/55/55134/tde-16032015-113613/pt-br.php},
}

@PhdThesis{Montanari,
  author        = {Raphael Montanari},
  school        = {Instituto de Ci{\^{e}}ncias Matem{\'{a}}ticas e de Computa{\c{c}}{\~{a}}o},
  title         = {Detec{\c{c}}{\~{a}}o e classifica{\c{c}}{\~{a}}o de objetos em imagens para rastreamento de ve{\'{\i}}culos},
  year          = {2015},
  abstract      = {A rob{\'{o}}tica {\'{e}} uma {\'{a}}rea multidisciplinar que cresce continuamente com a contribui{\c{c}}{\~{a}}o do avan{\c{c}}o cient{\'{i}}fico e aumento frequente do poder computacional do hardware. As pesquisas em rob{\'{o}}tica est{\~{a}}o divididas em diversas linhas de investiga{\c{c}}{\~{a}}o. A vis{\~{a}}o computacional {\'{e}} uma das linhas de pesquisa de grande interesse devido {\`{a}} farta variedade de m{\'{e}}todos e t{\'{e}}cnicas oferecidas. Um dos maiores desafios para os rob{\^{o}}s {\'{e}} descobrir e analisar o ambiente em que est{\~{a}}o inseridos. Dentre os principais sensores que podem ser utilizados, as c{\^{a}}meras digitais oferecem um bom benef{\'{i}}cio: podem ser leves, pequenas e baratas, caracter{\'{i}}sticas fundamentais para alguns rob{\^{o}}s. Este trabalho prop{\~{o}}e o desenvolvimento e an{\'{a}}lise de um sistema de vis{\~{a}}o computacional para rastrear ve{\'{i}}culos usando sistemas de detec{\c{c}}{\~{a}}o e classifica{\c{c}}{\~{a}}o de segmentos em imagens. Para atingir os objetivos s{\~{a}}o investigados m{\'{e}}todos de extra{\c{c}}{\~{a}}o de informa{\c{c}}{\~{o}}es das imagens, modelos de aten{\c{c}}{\~{a}}o visual e modelos de aprendizado bioinspirados para detec{\c{c}}{\~{a}}o e classifica{\c{c}}{\~{a}}o de ve{\'{i}}culos. Para a tarefa de aten{\c{c}}{\~{a}}o visual foram utilizadas as t{\'{e}}cnicas de gera{\c{c}}{\~{a}}o de mapas de sali{\^{e}}ncia iNVT e VOCUS2, enquanto que para classifica{\c{c}}{\~{a}}o foi empregada a t{\'{e}}cnicas bag-of-features e finalmente, para o rastreamento do ve{\'{i}}culo especificado, durante seu percurso em uma rodovia, foi adotada a t{\'{e}}cnica Camshift com filtro de Kalman. O sistema desenvolvido foi implementado com um rob{\^{o}} a{\'{e}}reo e testado com imagens reais contendo diferentes ve{\'{i}}culos em uma rodovia e os resultados de classifica{\c{c}}{\~{a}}o e rastreamento obtidos foram muito satisfat{\'{o}}rios.},
  doi           = {10.11606/d.55.2016.tde-08012016-113715},
  file          = {:C$\backslash$:/Users/guilo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Montanari - 2015 - Detec{\c{c}}{\~{a}}o e classifica{\c{c}}{\~{a}}o de objetos em imagens para rastreamento de ve{\'{i}}culos.pdf:pdf},
  keywords      = {Aprendizado de m{\'{a}}quina,Aprendizado de m{\'{a}}quina.,Aten{\c{c}}{\~{a}}o visual,Rob{\'{o}}tica,Vis{\~{a}}o computacional},
  mendeley-tags = {Aprendizado de m{\'{a}}quina,Aten{\c{c}}{\~{a}}o visual,Rob{\'{o}}tica,Vis{\~{a}}o computacional},
  pages         = {77},
  publisher     = {Universidade de Sao Paulo, Agencia {USP} de Gestao da Informacao Academica ({AGUIA})},
  url           = {https://www.teses.usp.br/teses/disponiveis/55/55134/tde-08012016-113715/pt-br.php},
}

@PhdThesis{Paiva,
  author        = {Jos{\'{e}} Gustavo de Souza Paiva},
  school        = {Instituto de Ci{\^{e}}ncias Matem{\'{a}}ticas e de Computa{\c{c}}{\~{a}}o},
  title         = {T{\'{e}}cnicas computacionais de apoio {\`{a}} classifica{\c{c}}{\~{a}}o visual de imagens e outros dados},
  year          = {2012},
  abstract      = {O processo autom{\'{a}}tico de classifica{\c{c}}{\~{a}}o de dados em geral, e em particular de classifica{\c{c}}{\~{a}}o de imagens, {\'{e}} uma tarefa computacionalmente intensiva e vari{\'{a}}vel em termos de precis{\~{a}}o, sendo consideravelmente dependente da configura{\c{c}}{\~{a}}o do classificador e da representa{\c{c}}{\~{a}}o dos dados utilizada. Muitos dos fatores que afetam uma adequada aplica{\c{c}}{\~{a}}o dos m{\'{e}}todos de classifica{\c{c}}{\~{a}}o ou categoriza{\c{c}}{\~{a}}o para imagens apontam para a necessidade de uma maior interfer{\^{e}}ncia do usu{\'{a}}rio no processo. Para isso s{\~{a}}o necess{\'{a}}rias mais ferramentas de apoio {\`{a}}s v{\'{a}}rias etapas do processo de classifica{\c{c}}{\~{a}}o, tais como, mas n{\~{a}}o limitadas, a extra{\c{c}}{\~{a}}o de caracter{\'{i}}sticas, a parametriza{\c{c}}{\~{a}}o dos algoritmos de classifica{\c{c}}{\~{a}}o e a escolha de inst{\^{a}}ncias de treinamento adequadas. Este doutorado apresenta uma metodologia para Classifica{\c{c}}{\~{a}}o Visual de Imagens, baseada na inser{\c{c}}{\~{a}}o do usu{\'{a}}rio no processo de classifica{\c{c}}{\~{a}}o autom{\'{a}}tica atrav{\'{e}}s do uso de t{\'{e}}cnicas de visualiza{\c{c}}{\~{a}}o. A ideia {\'{e}} permitir que o usu{\'{a}}rio participe de todos os passos da classifica{\c{c}}{\~{a}}o de determinada cole{\c{c}}{\~{a}}o, realizando ajustes e consequentemente melhorando os resultados de acordo com suas necessidades. Um estudo de diversas t{\'{e}}cnicas de visualiza{\c{c}}{\~{a}}o candidatas para a tarefa {\'{e}} apresentado, com destaque para as {\'{a}}rvores de similaridade, sendo apresentadas melhorias do algoritmo de constru{\c{c}}{\~{a}}o em termos de escalabilidade visual e de tempo de processamento. Adicionalmente, uma metodologia de redu{\c{c}}{\~{a}}o de dimensionalidade visual semi-supervisionada {\'{e}} apresentada para apoiar, pela utiliza{\c{c}}{\~{a}}o de ferramentas visuais, a cria{\c{c}}{\~{a}}o de espa{\c{c}}os reduzidos que melhorem as caracter{\'{i}}sticas de segrega{\c{c}}{\~{a}}o do conjunto original de caracter{\'{i}}sticas. A principal contribui{\c{c}}{\~{a}}o do trabalho {\'{e}} um sistema de classifica{\c{c}}{\~{a}}o visual incremental que incorpora todos os passos da metodologia proposta, oferecendo ferramentas interativas e visuais que permitem a interfer{\^{e}}ncia do usu{\'{a}}rio na classifica{\c{c}}{\~{a}}o de cole{\c{c}}{\~{o}}es incrementais com configura{\c{c}}{\~{a}}o de classes vari{\'{a}}vel. Isso possibilita a utiliza{\c{c}}{\~{a}}o do conhecimento do ser humano na constru{\c{c}}{\~{a}}o de classificadores que se adequem a diferentes necessidades dos usu{\'{a}}rios em diferentes cen{\'{a}}rios, produzindo resultados satisfat{\'{o}}rios para cole{\c{c}}{\~{o}}es de dados diversas. O foco desta tese {\'{e}} em categoriza{\c{c}}{\~{a}}o de cole{\c{c}}{\~{o}}es de imagens, com exemplos tamb{\'{e}}m para conjuntos de dados textuais.},
  doi           = {10.11606/t.55.2012.tde-02042013-084718},
  file          = {:C$\backslash$:/Users/guilo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Paiva - 2012 - T{\'{e}}cnicas computacionais de apoio {\`{a}} classifica{\c{c}}{\~{a}}o visual de imagens e outros dados.pdf:pdf},
  keywords      = {Classifica{\c{c}}{\~{a}}o visual de dados,Redu{\c{c}}{\~{a}}o de dimensionalidade,Visualiza{\c{c}}{\~{a}}o de informa{\c{c}}{\~{a}}o},
  mendeley-tags = {Classifica{\c{c}}{\~{a}}o visual de dados,Redu{\c{c}}{\~{a}}o de dimensionalidade,Visualiza{\c{c}}{\~{a}}o de informa{\c{c}}{\~{a}}o},
  pages         = {190},
  publisher     = {Universidade de Sao Paulo, Agencia {USP} de Gestao da Informacao Academica ({AGUIA})},
  url           = {https://www.teses.usp.br/teses/disponiveis/55/55134/tde-02042013-084718/pt-br.php},
}

@Article{Uijlings_2013,
  author    = {J. R. R. Uijlings and K. E. A. van de Sande and T. Gevers and A. W. M. Smeulders},
  journal   = {International Journal of Computer Vision},
  title     = {{Selective Search for Object Recognition}},
  year      = {2013},
  issn      = {09205691},
  month     = {apr},
  number    = {2},
  pages     = {154--171},
  volume    = {104},
  abstract  = {This paper addresses the problem of generating possible object locations for use in object recognition. We introduce selective search which combines the strength of both an exhaustive search and segmentation. Like segmentation, we use the image structure to guide our sampling process. Like exhaustive search, we aim to capture all possible object locations. Instead of a single technique to generate possible object locations, we diversify our search and use a variety of complementary image partitionings to deal with as many image conditions as possible. Our selective search results in a small set of data-driven, class-independent, high quality locations, yielding 99 {\%} recall and a Mean Average Best Overlap of 0.879 at 10,097 locations. The reduced number of locations compared to an exhaustive search enables the use of stronger machine learning techniques and stronger appearance models for object recognition. In this paper we show that our selective search enables the use of the powerful Bag-of-Words model for recognition. The selective search software is made publicly available (Software: http://disi.unitn.it/{\~{}}uijlings/SelectiveSearch. html). {\textcopyright} 2013 Springer Science+Business Media New York.},
  doi       = {10.1007/s11263-013-0620-5},
  file      = {:C$\backslash$:/Users/guilo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Uijlings et al. - 2013 - Selective Search for Object Recognition.pdf:pdf},
  keywords  = {()},
  publisher = {Springer Science and Business Media {LLC}},
  url       = {https://doi.org/10.1007/s11263-013-0620-5 http://disi.unitn.it/{~}uijlings/SelectiveSearch.html},
}

@PhdThesis{Silva,
  author        = {N{\'{u}}bia Rosa da Silva},
  school        = {Instituto de Ci{\^{e}}ncias Matem{\'{a}}ticas e de Computa{\c{c}}{\~{a}}o},
  title         = {Reconhecimento de padr{\~{o}}es heterog{\^{e}}neos e suas aplica{\c{c}}{\~{o}}es em biologia e nanotecnologia},
  year          = {2015},
  abstract      = {O reconhecimento de padr{\~{o}}es de textura em imagens tem sido uma importante ferramenta na {\'{a}}rea de vis{\~{a}}o computacional. Isso porque o atributo textura pode revelar caracter{\'{i}}sticas intr{\'{i}}nsecas, tornando poss{\'{i}}vel a classifica{\c{c}}{\~{a}}o de um conjunto de imagens semelhantes. Embora a textura seja estudada h{\'{a}} mais de meio s{\'{e}}culo, ainda n{\~{a}}o existe um consenso sobre sua defini{\c{c}}{\~{a}}o e nem mesmo um m{\'{e}}todo de extra{\c{c}}{\~{a}}o de caracter{\'{i}}sticas de textura que seja eficiente para todos os tipos de imagens. Al{\'{e}}m disso, os m{\'{e}}todos da literatura analisam os padr{\~{o}}es de textura de maneira global, considerando que uma imagem apresente um conjunto de micropadr{\~{o}}es que formam um {\'{u}}nico padr{\~{a}}o global ou homog{\^{e}}neo de textura na imagem. No entanto, alguns tipos de imagens apresentam heterogeneidade em sua composi{\c{c}}{\~{a}}o, ou seja, o conjunto de micropadr{\~{o}}es na imagem {\'{e}} respons{\'{a}}vel por formar mais de um padr{\~{a}}o de textura dentro da mesma imagem. Esse tipo de imagens levou ao prop{\'{o}}sito de investiga{\c{c}}{\~{a}}o deste trabalho. Independentemente do m{\'{e}}todo de extra{\c{c}}{\~{a}}o de caracter{\'{i}}stica utilizado, considerar a heterogeneidade do padr{\~{a}}o de textura em uma imagem leva a uma melhor representa{\c{c}}{\~{a}}o de suas caracter{\'{i}}sticas. Para melhorar a an{\'{a}}lise de padr{\~{o}}es heterog{\^{e}}neos de textura, tr{\^{e}}s abordagens s{\~{a}}o propostas: (i) lazy-patch, (ii) combina{\c{c}}{\~{a}}o de modelos e (iii) modelagem da textura por meio de aut{\^{o}}matos celulares inspirados em corros{\~{a}}o alveolar. Os resultados ao aplicar essas abordagens em diferentes conjuntos de imagens de biologia e nanotecnologia, mostraram que a an{\'{a}}lise de padr{\~{o}}es heterog{\^{e}}neos resulta em melhor representatividade de imagens que possuem padr{\~{o}}es heterog{\^{e}}neos de textura em sua composi{\c{c}}{\~{a}}o.},
  doi           = {10.11606/t.55.2016.tde-30032016-145929},
  file          = {:C$\backslash$:/Users/guilo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Silva - 2015 - Reconhecimento de padr{\~{o}}es heterog{\^{e}}neos e suas aplica{\c{c}}{\~{o}}es em biologia e nanotecnologia.pdf:pdf},
  keywords      = {An{\'{a}}lise de imagens,Reconhecimento de padr{\~{o}}es,Textura heterog{\^{e}}nea,Vis{\~{a}}o computacional},
  mendeley-tags = {An{\'{a}}lise de imagens,Reconhecimento de padr{\~{o}}es,Textura heterog{\^{e}}nea,Vis{\~{a}}o computacional},
  pages         = {128},

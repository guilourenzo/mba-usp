}

@PhdThesis{Roncatti,
  author        = {Marco Aurelio Roncatti},
  school        = {Instituto de Ci{\^{e}}ncias Matem{\'{a}}ticas e de Computa{\c{c}}{\~{a}}o},
  title         = {Avalia{\c{c}}{\~{a}}o de m{\'{e}}todos {\'{o}}timos e sub{\'{o}}timos de sele{\c{c}}{\~{a}}o de caracter{\'{\i}}sticas de texturas em imagens},
  year          = {2008},
  abstract      = {Caracter{\'{i}}sticas de texturas atuam como bons descritores de imagens e podem ser empregadas em diversos problemas, como classifica{\c{c}}{\~{a}}o e segmenta{\c{c}}{\~{a}}o. Por{\'{e}}m, quando o n{\'{u}}mero de caracter{\'{i}}sticas {\'{e}} muito elevado, o reconhecimento de padr{\~{o}}es pode ser prejudicado. A sele{\c{c}}{\~{a}}o de caracter{\'{i}}sticas contribui para a solu{\c{c}}{\~{a}}o desse problema, podendo ser empregada tanto para redu{\c{c}}{\~{a}}o da dimensionalidade como tamb{\'{e}}m para descobrir quais as melhores caracter{\'{i}}sticas de texturas para o tipo de imagem analisada. O objetivo deste trabalho {\'{e}} avaliar m{\'{e}}todos {\'{o}}timos e sub{\'{o}}timos de sele{\c{c}}{\~{a}}o de caracter{\'{i}}sticas em problemas que envolvem texturas de imagens. Os algoritmos de sele{\c{c}}{\~{a}}o avaliados foram o branch and bound, a busca exaustiva e o sequential oating forward selection (SFFS). As fun{\c{c}}{\~{o}}es crit{\'{e}}rio empregadas na sele{\c{c}}{\~{a}}o foram a dist{\^{a}}ncia de Jeffries-Matusita e a taxa de acerto do classificador de dist{\^{a}}ncia m{\'{i}}nima (CDM). As caracter{\'{i}}sticas de texturas empregadas nos experimentos foram obtidas com estat{\'{i}}sticas de primeira ordem, matrizes de co-ocorr{\^{e}}ncia e filtros de Gabor. Os experimentos realizados foram a classifica{\c{c}}{\~{a}}o de regi{\^{o}}es de uma foto a{\'{e}}rea de planta{\c{c}}{\~{a}}o de eucalipto, a segmenta{\c{c}}{\~{a}}o n{\~{a}}o-supervisionada de mosaicos de texturas de Brodatz e a segmenta{\c{c}}{\~{a}}o supervisionada de imagens m{\'{e}}dicas (MRI do c{\'{e}}rebro). O branch and bound {\'{e}} um algoritmo {\'{o}}timo e mais efiiente do que a busca exaustiva na maioria dos casos. Por{\'{e}}m, continua sendo um algoritmo lento. Este trabalho apresenta uma nova estrat{\'{e}}gia para o branch and bound, nomeada floresta, que melhorou significativamente a efici{\^{e}}ncia do algoritmo. A avalia{\c{c}}{\~{a}}o dos m{\'{e}}todos de sele{\c{c}}{\~{a}}o de caracter{\'{i}}sticas mostrou que os melhores subconjuntos foram aqueles obtidos com o uso da taxa de acerto do CDM. A busca exaustiva e o branch and bound, mesmo com a estrat{\'{e}}gia floresta, foram considerados invi{\'{a}}veis devido ao alto tempo de processamento nos casos em que o n{\'{u}}mero de caracter{\'{i}}stica {\'{e}} muito grande. O SFFS apresentou os melhores resultados, pois, al{\'{e}}m de mais r{\'{a}}pido, encontrou as solu{\c{c}}{\~{o}}es {\'{o}}timas ou pr{\'{o}}ximas das {\'{o}}timas. P{\^{o}}de-se concluir tamb{\'{e}}m que a precis{\~{a}}o no reconhecimento de padr{\~{o}}es aumenta com a redu{\c{c}}{\~{a}}o do n{\'{u}}mero de caracter{\'{i}}sticas e que os melhores subconjuntos freq{\"{u}}entemente s{\~{a}}o formados por caracter{\'{i}}sticas de texturas obtidas com t{\'{e}}cnicas diferentes.},
  doi           = {10.11606/d.55.2008.tde-26082008-170000},
  file          = {:C$\backslash$:/Users/guilo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Roncatti - 2008 - Avalia{\c{c}}{\~{a}}o de m{\'{e}}todos {\'{o}}timos e sub{\'{o}}timos de sele{\c{c}}{\~{a}}o de caracter{\'{i}}sticas de texturas em imagens.pdf:pdf},
  keywords      = {Branch and bound,Reconhecimento de padr{\~{o}}es,Sele{\c{c}}{\~{a}}o de caracter{\'{i}}sticas,Sequential floating forward selection,Texturas},
  mendeley-tags = {Branch and bound,Reconhecimento de padr{\~{o}}es,Sele{\c{c}}{\~{a}}o de caracter{\'{i}}sticas,Sequential floating forward selection,Texturas},
  pages         = {110},
  publisher     = {Universidade de Sao Paulo, Agencia {USP} de Gestao da Informacao Academica ({AGUIA})},
  url           = {https://www.teses.usp.br/teses/disponiveis/55/55134/tde-26082008-170000/pt-br.php},
}

@InProceedings{Mayer_2017,
  author    = {Felix Mayer and Martin Steinebach},
  booktitle = {Proceedings of the 12th International Conference on Availability, Reliability and Security},
  title     = {{Forensic Image Inspection Assisted by Deep Learning}},
  year      = {2017},
  month     = {aug},
  publisher = {{ACM}},
  volume    = {Part F1305},
  abstract  = {Investigations on the charge of possessing child pornography usually require manual forensic image inspection in order to collect evidence. When storage devices are confiscated, law enforcement authorities are hence often faced with massive image datasets which have to be screened within a limited time frame. As the ability to concentrate and time are highly limited factors of a human investigator, we believe that intelligent algorithms can effectively assist the inspection process by rearranging images based on their content. Thus, more relevant images can be discovered within a shorter time frame, which is of special importance in time-critical investigations of triage character. While currently employed techniques are based on black- And whitelisting of known images, we propose to use deep learning algorithms trained for the detection of pornographic imagery, as they are able to identify new content. In our approach, we evaluated three state-of-the-art neural networks for the detection of pornographic images and employed them to rearrange simulated datasets of 1 million images containing a small fraction of pornographic content. The rearrangement of images according to their content allows a much earlier detection of relevant images during the actual manual inspection of the dataset, especially when the percentage of relevant images is low. With our approach, the first relevant image could be discovered between positions 8 and 9 in the rearranged list on average. Without using our approach of image rearrangement, the first relevant image was discovered at position 1,463 on average.},
  doi       = {10.1145/3098954.3104051},
  file      = {:C$\backslash$:/Users/guilo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mayer, Steinebach - 2017 - Forensic image inspection assisted by deep learning.pdf:pdf},
  isbn      = {9781450352574},
  keywords  = {Content-based image retrieval,Deep learning,Information retrieval,Ranking,Text tagging},
}


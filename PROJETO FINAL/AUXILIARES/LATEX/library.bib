   volume = {4},
   url = {http://www.nowpublishers.com/article/Details/INR-017},
   year = {2010},
}
@inproceedings{Lenarduzzi2016,
   abstract = {Context: One of the most important steps of the Lean Startupmethodology is the definition of Minimum Viable Product (MVP), needed to start the learning process by integrating the early adopters' feedbacks as soon as possible. Objective: This study aims at identifying the common definitions of MVP proposed and the key factors identified to help entrepreneurs efficiently define their MVP, reducing errors due to unconsidered unknown factors. Method: We identified the MVP definitions and key factors by means of a systematic mapping study, defining the research questions and the protocol to be used. We selected the bibliographic sources, the keywords, and the selection criteria for searching the relevant papers. Results: We found 97 articles and, through inclusion and exclusion criteria, removed 75 articles, which reduced the total to 22 at the end of the process. The results are a classification schema for characterizing the definition of Minimum Viable Product in Lean Startups and a set of common key factors identified in the MVP definitions. Conclusion: The identified key factors are related to technical characteristics of the product as well as market and customer aspects. We found a positive improvement of the state of the art of MVP and the definition of Minimum.},
   author = {Valentina Lenarduzzi and Davide Taibi},
   doi = {10.1109/SEAA.2016.56},
   isbn = {978-1-5090-2820-7},
   journal = {2016 42th Euromicro Conference on Software Engineering and Advanced Applications (SEAA)},
   keywords = {Entrepreneurship,Lean Startup,Minimum Viable Product,Startup},
   month = {8},
   pages = {112-119},
   publisher = {IEEE},
   title = {MVP Explained: A Systematic Mapping Study on the Definitions of Minimal Viable Product},
   url = {http://ieeexplore.ieee.org/document/7592786/},
   year = {2016},
}

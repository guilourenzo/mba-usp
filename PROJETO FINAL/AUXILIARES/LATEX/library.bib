@InProceedings{Freitas2019,
  author    = {Pedro V. A. de Freitas and Gabriel N. P. dos Santos and Antonio J. G. Busson and {\'{A}}lan L. V. Guedes and S{\'{e}}rgio Colcher},
  booktitle = {Proceedings of the 25th Brazillian Symposium on Multimedia and the Web - {WebMedia} {\textquotesingle}19},
